<%- include('partials/header') -%>
<div class="container">
  <div class="row mt-5">
    <div class="col-6">
      <div>
        <% if (employer) { %>
        <p>Name: <%= employer.name %></p>
        <p>Company: <%= employer.companyName %></p>
        <p>Location: <%= employer.location %></p>
        <p>Number: <%= employer.number %></p>
        <p>About The Company: <%= employer.bioBlurb %></p>
        <% } %> <% if (seeker) { %> <% if (seeker.image) { %>
        <p><%= seeker.image %></p>
        <% } %>
        <p>Name: <%= seeker.name %></p>
        <p>Location: <%= seeker.location %></p>
        <p>Qualificiations: <%= seeker.quals %></p>
        <p>About Me: <%= seeker.bioBlurb %></p>
        <% } %>
      </div>
    </div>
  </div>
</div>
<%- include('partials/footer') -%>
